{
  "name": "overkiz-client",
  "displayName": "Overkiz API Client",
  "version": "1.0.15",
  "description": "Overkiz API client plugin for Node",
  "author": "Romain DUBOC",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/dubocr/overkiz-client.git"
  },
  "bugs": {
    "url": "http://github.com/dubocr/overkiz-client/issues"
  },
  "engines": {
    "node": ">=10.17.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**.ts",
    "test": "npm run build && node --es-module-specifier-resolution=node ./dist/test.js",
    "clean": "rimraf ./dist",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build && npm version patch --m 'Release %s'",
    "postpublish": "npm run clean"
  },
  "keywords": [
    "tahoma",
    "cozytouch"
  ],
  "homepage": "https://github.com/dubocr/overkiz-client#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "bonjour": "^3.5.0",
    "events": "^3.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^14.17.5",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "dotenv": "^8.6.0",
    "eslint": "^7.30.0",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
